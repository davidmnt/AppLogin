<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LoginCorrectoActivity">


    <TextView
        android:id="@+id/textLogin"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"
        android:gravity="start"
        android:padding="10dp"
        android:textColor="@color/black"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/perfilFoto"
        android:layout_width="100dp"
        android:layout_height="110dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/perfil"
        app:layout_constraintStart_toStartOf="@+id/textLogin"
        app:layout_constraintTop_toBottomOf="@+id/textLogin" />

    <TextView
        android:id="@+id/NumeroPublicaciones"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="5"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/publicaciones"
        app:layout_constraintStart_toEndOf="@+id/perfilFoto"
        app:layout_constraintTop_toTopOf="@+id/perfilFoto" />

    <TextView
        android:id="@+id/publicaciones"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="Publicaciones"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/perfilFoto"
        app:layout_constraintStart_toEndOf="@+id/perfilFoto"
        app:layout_constraintTop_toTopOf="@+id/perfilFoto" />

    <TextView
        android:id="@+id/NumeroSeguidores"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="1,5M"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/publicaciones"
        app:layout_constraintStart_toEndOf="@+id/NumeroPublicaciones"
        app:layout_constraintTop_toTopOf="@+id/perfilFoto" />

    <TextView
        android:id="@+id/seguidores"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Seguidores"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/perfilFoto"
        app:layout_constraintStart_toEndOf="@+id/publicaciones"
        app:layout_constraintTop_toTopOf="@+id/perfilFoto" />

    <TextView
        android:id="@+id/NumeroSiguiendo"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="45"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/publicaciones"
        app:layout_constraintStart_toEndOf="@+id/NumeroSeguidores"
        app:layout_constraintTop_toTopOf="@+id/perfilFoto" />

    <TextView
        android:id="@+id/Siguiendo"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Siguiendo"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/perfilFoto"
        app:layout_constraintStart_toEndOf="@+id/seguidores"
        app:layout_constraintTop_toTopOf="@+id/perfilFoto" />

    <ImageView
        android:id="@+id/Foto1"
        android:layout_width="200dp"
        android:layout_height="120dp"
        android:src="@drawable/ronnie"
        app:layout_constraintTop_toBottomOf="@+id/perfilFoto"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="30dp"
        />

    <ImageView
        android:id="@+id/Foto2"
        android:layout_width="200dp"
        android:layout_height="120dp"
        android:src="@drawable/ronnie"
        app:layout_constraintTop_toBottomOf="@+id/perfilFoto"
        app:layout_constraintStart_toEndOf="@+id/Foto1"
        android:layout_marginTop="30dp"/>

    <ImageView
        android:id="@+id/Foto3"
        android:layout_width="200dp"
        android:layout_height="120dp"
        android:src="@drawable/ronnie"
        app:layout_constraintTop_toBottomOf="@+id/Foto1"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="10dp"
        />

    <ImageView
        android:id="@+id/Foto4"
        android:layout_width="200dp"
        android:layout_height="120dp"
        android:src="@drawable/ronnie"
        app:layout_constraintTop_toBottomOf="@+id/Foto2"
        app:layout_constraintStart_toEndOf="@+id/Foto1"
        android:layout_marginTop="10dp"
        />

    <ImageView
        android:id="@+id/Foto5"
        android:layout_width="200dp"
        android:layout_height="120dp"
        android:src="@drawable/ronnie"
        app:layout_constraintTop_toBottomOf="@+id/Foto3"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="10dp"
        />



    <TextView
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:background="@color/white"
        app:layout_constraintTop_toTopOf="@+id/lupa"
        />

    <ImageView
        android:id="@+id/casa"
        android:layout_width="80dp"
        android:layout_height="45dp"
        android:layout_marginBottom="5dp"
        android:src="@drawable/casa"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/lupa"
        android:layout_width="80dp"
        android:layout_height="45dp"
        android:layout_marginBottom="5dp"
        android:src="@drawable/lupa"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/casa" />

    <ImageView
        android:id="@+id/publicar"
        android:layout_width="80dp"
        android:layout_height="45dp"
        android:layout_marginBottom="5dp"
        android:src="@drawable/publicar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/lupa" />

    <ImageView
        android:id="@+id/reels"
        android:layout_width="80dp"
        android:layout_height="45dp"
        android:layout_marginBottom="5dp"
        android:src="@drawable/reels"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/publicar" />

    <ImageView
        android:id="@+id/perfil"
        android:layout_width="80dp"
        android:layout_height="45dp"
        android:layout_marginBottom="5dp"
        android:src="@drawable/perfil"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/reels" />


</androidx.constraintlayout.widget.ConstraintLayout>